1)
#include<stdio.h>
int main() {
    int i;
    for(i=1;i<=10;i++){
    printf("%d ",i);
    }
}

2)
#include<stdio.h>
int main() {
    int i;
    int sum = 0;
    for(i=1;i<=10;i++){
        sum=sum+i;
    }
    printf("The sum is %d ",sum);
    }

4)
#include<stdio.h>
int main() {
    int i;
    int n;
    int cube=1;
    printf("\nInput number of terms :");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
          cube=i*i*i;
          printf("\nNumber is: %d cube is %d",i,cube);
    }}
   
5)
#include<stdio.h>
int main() {
    int i,o;
    for(o=1;o<=4;o++){
        for(i=1;i<=o;i++)
        {
            printf("%d ",o);
        }
     printf("\n");   
    }}

1)
#include<stdio.h>
int main() {
    int i,j;
    printf("\nTest data :");
    scanf("%d %d",&i,&j);
    if(i==j)
    {
        printf("\nNumber 1  and Number 2 are equal");
    }
    else
    {
        printf("\nNumber 1 and number 2 are not equal");
    }

    }
    
2)
#include<stdio.h>
int main() {
    int i;
    printf("\nTest data :");
    scanf("%d",&i);
    if(i>0)
    {
        printf("\n%d is a positive number",i);
    }
    else
    {
        printf("\n%d is a negative number",i);

    }

    }
    
3)
#include<stdio.h>
int main() {
    int a,b,c;
    printf("\nTest data :");
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);

    if(a==b && a==c && b==c)
    {
        printf("\nThis is an equilateral triangle");
        
    }
        else if(a==b||a==c||b==c)
    {
        printf("\nThis is an iscoseles triangle");
    }
    else
    {
        printf("\nThis is an scalene triangle");

    }

    }
    
    
    
