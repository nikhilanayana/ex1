#include<stdio.h>
int main() {
    int i;
    for(i=1;i<=10;i++){
    printf("%d ",i);
    }
}

2)
#include<stdio.h>
int main() {
    int i;
    int sum = 0;
    for(i=1;i<=10;i++){
        sum=sum+i;
    }
    printf("The sum is %d ",sum);
    }

4)
#include<stdio.h>
int main() {
    int i;
    int n;
    int cube=1;
    printf("\nInput number of terms :");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
          cube=i*i*i;
          printf("\nNumber is: %d cube is %d",i,cube);
    }}
   
5)
#include<stdio.h>
int main() {
    int i,o;
    for(o=1;o<=4;o++){
        for(i=1;i<=o;i++)
        {
            printf("%d ",o);
        }
     printf("\n");   
    }}

1)
#include<stdio.h>
int main() {
    int i,j;
    printf("\nTest data :");
    scanf("%d %d",&i,&j);
    if(i==j)
    {
        printf("\nNumber 1  and Number 2 are equal");
    }
    else
    {
        printf("\nNumber 1 and number 2 are not equal");
    }

    }
