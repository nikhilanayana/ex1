1)
#include<stdio.h>
int main() {
    int i;
    for(i=1;i<=10;i++){
    printf("%d ",i);
    }
}

2)
#include<stdio.h>
int main() {
    int i;
    int sum = 0;
    for(i=1;i<=10;i++){
        sum=sum+i;
    }
    printf("The sum is %d ",sum);
    }

3)
#include<stdio.h>
int main() {
    int i,o;
    for(o=1;o<=4;o++){
        for(i=1;i<=o;i++)
        {
            printf("%d ",o);
        }
     printf("\n");   
    }}
