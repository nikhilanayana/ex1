1)
#include<stdio.h>
int main() {
    int i;
    for(i=1;i<=10;i++){
    printf("%d ",i);
    }
}

2)
#include<stdio.h>
int main() {
    int i;
    int sum = 0;
    for(i=1;i<=10;i++){
        sum=sum+i;
    }
    printf("The sum is %d ",sum);
    }

3)
#include<stdio.h>
int main() {
    int i;
    int n;
    int cube=1;
    printf("\nInput number of terms :");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
          cube=i*i*i;
          printf("\nNumber is: %d cube is %d",i,cube);
    }}
   
4)
#include<stdio.h>
int main() {
    int i,o;
    for(o=1;o<=4;o++){
        for(i=1;i<=o;i++)
        {
            printf("%d ",o);
        }
     printf("\n");   
    }}
